{
  "name": "aws-lambda-typescript",
  "version": "1.0.0",
  "description": "AWS Lambda function with TypeScript",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "deploy": "npm run build && sam deploy",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "aws",
    "lambda",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@types/aws-lambda": "^8.10.130"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^14.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.0",
    "typescript": "~5.5.0",
    "typescript-eslint": "^7.0.0"
  }
}
